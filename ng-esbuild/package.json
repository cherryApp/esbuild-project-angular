{
  "name": "esbuild-ng-cli",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "node ./esbuild",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "esbuild": "esbuild src/main.ts --bundle --outfile=dist/main.js --loader:.html=text",
    "esbuild:watch": "esbuild src/main.ts --bundle --outfile=dist/main.js --loader:.html=text --minify --watch --sourcemap",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "laravel-mix": "^6.0.39",
    "laravel-mix-code-splitter": "^1.0.5",
    "routing": "0.0.2",
    "rxjs": "~7.5.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.1",
    "@swc/core": "^1.2.127",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^17.0.8",
    "chokidar": "^3.5.2",
    "esbuild": "^0.14.10",
    "esbuild-plugin-tsc": "^0.3.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "typescript": "^4.5.4",
    "ws": "^8.4.0"
  }
}
